
pipeline{
    agent any

    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/mdabuhasnat/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                sh "/usr/share/apache-maven/bin/mvn clean package"
            }
        }
    }
}
